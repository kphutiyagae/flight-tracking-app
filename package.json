{
  "name": "flight-tracking-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": " npx eslint . --ext .ts",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "tailwind": "npx tailwindcss -i ./src/stylesheets/styles.css -o ./dist/stylesheets/styles.css",
    "tailwind-watch": "npx tailwindcss -i ./src/stylesheets/styles.css -o ./dist/stylesheets/styles.css --watch"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.7.0",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "leaflet": "^1.9.3",
    "rxjs": "^7.8.0"
  }
}